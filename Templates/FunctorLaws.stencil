import XCTest
@testable import FunctionalKit
import SwiftCheck

{% for type in types.all|annotated:"functor"|annotated:"simpleFunctor" %}
//MARK: {{ type.name }}
// Identity Law
    func testIdentity() {
        property("{{ type.name }} - Functor Laws - Identity") <- forAll { (x: Int) in
            return {{ type.name }}.unfold({ $0(x) }).map(fidentity) == fidentity({{ type.name }}.unfold({ $0(x) }))
        }
    }
{% endfor %}
